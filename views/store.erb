	<!-- 場所url -->

	<b>google map</b><br>
	<input type="text" name="map_url"><br>

	<!-- 電話番号 -->

	<b>電話番号(半角・ハイフンなし)</b><br>
	<input type="text" name="open"><br>

	

	<!-- 座席の数 -->
	<!-- ここはjsかなにかで -->

	<!-- 一言コメント！ -->
	<b>お店のPR</b><br>
	<textarea cols="40" rows="5" name="comment"></textarea><br>
	<!-- 残り何文字かわかるように -->
  <input type="submit" value="登録する">

</form>






